{
  "name": "sharedstreets",
  "version": "0.15.3",
  "description": "SharedStreets, a 'digital commons' for the street",
  "exports": "./build/src/index.js",
  "bin": {
    "shst": "./bin/run"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/build",
    "/bin",
    "/src"
  ],
  "type": "module",
  "oclif": {
    "commands": "./build/src/commands",
    "bin": "shst",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "pretest": "tsc",
    "prepack": "rm -rf build && tsc && mkdir -p build/src/proto && cp src/proto/*.js build/src/proto/ && oclif manifest",
    "test": "tsc && colortape build/test_core.js",
    "test-match": "tsc && colortape build/test_match.js",
    "test-graph": "tsc && colortape build/test_graph.js",
    "docs": "tsc && documentation readme build/index.js --section=API",
    "bench": "tsc && node bench.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharedstreets/sharedstreets-js.git"
  },
  "keywords": [
    "sharedstreets",
    "streets",
    "highway",
    "reference"
  ],
  "author": "SharedStreets",
  "contributors": [
    "Denis Carriere <@DenisCarriere>",
    "Kevin Webb <@kpwebb>",
    "Morgan Herlocker <@morganherlocker>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedstreets/sharedstreets-js/issues"
  },
  "homepage": "https://github.com/sharedstreets/sharedstreets-js#readme",
  "devDependencies": {
    "oclif": "^3.2.16",
    "@types/benchmark": "*",
    "@types/glob": "*",
    "@types/node": "*",
    "@types/tape": "*",
    "benchmark": "^2.1.4",
    "colortape": "^0.1.2",
    "documentation": "^14.0.0",
    "tap": "^16.3.0",
    "tape": "*",
    "ts-node": "^10.9.1",
    "tslint": "*",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@mapbox/sphericalmercator": "^1.2.0",
    "@oclif/command": "^1.8.18",
    "@oclif/config": "^1.18.5",
    "@oclif/plugin-help": "^5.1.15",
    "@turf/along": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/bearing": "^6.5.0",
    "@turf/buffer": "^6.5.0",
    "@turf/envelope": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/invariant": "^6.5.0",
    "@turf/length": "^6.5.0",
    "@turf/line-offset": "^6.5.0",
    "@turf/line-slice-along": "^6.5.0",
    "@turf/nearest-point-on-line": "^6.5.0",
    "@types/leveldown": "^4.0.3",
    "@types/levelup": "^5.1.2",
    "@types/osrm": "^5.25.2",
    "bignumber.js": "9.x",
    "chalk": "^4.1.2",
    "cli-progress": "^3.11.2",
    "const": "^1.0.0",
    "expand-tilde": "^2.0.2",
    "jkstra": "^0.1.1",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "node-fetch": "^3.2.10",
    "osrm": "^5.26.0",
    "rbush": "^3.0.1",
    "sharedstreets-pbf": "^0.8.0",
    "sharedstreets-types": "^1.3.1",
    "simple-statistics": "^7.7.6",
    "tslib": "^2.4.0",
    "uuid-by-string": "^4.0.0",
    "xml": "^1.0.1"
  }
}
